<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lebelle.javadevelopers">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_quad"
        android:label="@string/app_name2"
        android:roundIcon="@mipmap/ic_launcher_quad"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controllers.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controllers.ProfileActivity"
            android:label=" "
            android:launchMode="singleTop"
            android:parentActivityName=".controllers.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".controllers.MainActivity"/>
        </activity>
    </application>

</manifest>